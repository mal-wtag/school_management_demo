<h1>All Users</h1>
<div class="col-md-12">
  <%= link_to 'Create New User',new_user_path,:class=>"pull-right"%>
</div>


<div class="col-md-12">
  <table class="table">
    <thead class="thead-dark">
      <th>Name</th>
      <th>Type</th>
      <th>Phone</th>
      <th>Roll</th>
      <th>Address</th>
    <th colspan='3'>Action</th>
    </thead>
    <tbody>
  <% @users.each do |user|%>
        <tr>
          <td><%= user.name %></td>
          <td>Not implemented Yet</td>
          <td><%= user.phone %></td>
          <td><%= user.roll %></td>
          <td><%= user.address %></td>
          <td><%= link_to 'Show',user %></td>
          <td><%= link_to 'edit',edit_user_path(user) %></td>
          <td><%= link_to 'Delete',user, method: :delete, data:{confirm: 'Are you sure?'} %></td>
          <td></td>
          <td></td>
        </tr>
  <% end %>
    </tbody>
  </table>
</div>